<?xml version="1.0" encoding="UTF-8"?>
<newsItem
    xmlns="http://iptc.org/std/nar/2006-10-01/"
    xmlns:xs="http://www.w3.org/2001/XMLSchema"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://www.w3.org/ns/odrl/2/ ./ODRL22.xsd"
    xmlns:other="http://other.com/other-namespace"
    xmlns:o="http://www.w3.org/ns/odrl/2/"
    guid="simplest-test"
    standard="NewsML-G2"
    standardversion="2.20"
    conformance="power"
    xml:lang="en-GB">
    <catalogRef href="http://www.iptc.org/std/catalog/catalog.IPTC-G2-Standards_30.xml"/>
    <itemMeta>
        <itemClass qcode="ninat:text"/>
        <provider literal="reuters.com"/>
        <versionCreated>2019-09-09T08:00:00.000Z</versionCreated>
        <!-- CR00181/SGR325/EXT016: Original Representation, for permalink
        •   altLoc has @role: locRole:primary / mirror1 / mirror2 / ...
        •   altLoc has @type: locType:http / ftp / ...
        •   So include:
        •   commonPowerAttrs
        •   timeValidityAttrs
        •   @reposrole, @reposroleuri: == locRole:*
        •   @accesstype, @accesstypeuri: == locType:*
        -->
        <!-- Original Representation
@reposrole: Repository Role
@accesstype: Access Type
-->
        <origRep reposrole="locRole:permalink" accesstype="locType:http">http://example.com/reuters/my-item-id</origRep>
        <origRep reposrole="locRole:permalink" accesstype="locType:ftp">ftp://example.com/reuters?getItem=my-item-id</origRep>
    </itemMeta>
</newsItem>
