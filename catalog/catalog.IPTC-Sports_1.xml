<!-- File: http://www.iptc.org/std/catalog/catalog.IPTC-Sports_#.xml (# represents the version number of this file)

	This file provides a Catalog for G2-Standards as defined by the common News Architecture
	This catalog contains all currently existing IPTC maintained controlled vocabularies for SportsML 
	- see the explanations in the headers below.

	How to apply:
	When creating an XML instance of any kind of item for a G2-Standard do this:
	- either: copy and paste the <catalog> element from this file to the appropriate position in the item
	- or: add a <catalogRef> element to the item and apply the URL of this file (wherever it is stored)
		to the "href" attribute of the catalogRef element like: 
		<catalogRef href="http://mydomain.org/files/catalog.IPTC-G2-Sports_99.xml" />

	This file is provided as is by the International Press Telecommunication Council, IPTC - www.iptc.org
	History / change log:
	v 1 2016-05-23: initial version, reflects vocabularies created for SportsML 3.0
-->	

<catalog xmlns="http://iptc.org/std/nar/2006-10-01/" additionalInfo="http://www.iptc.org/goto?G2cataloginfo">

	<!-- General sport vocabularies appicable to all sports -->
	
	<scheme alias="spactionclass" uri="http://cv.iptc.org/newscodes/spactionclass/">
		<definition xml:lang="en-GB">High-level categories of on-field action in all sports.</definition>
		<name xml:lang="en-GB">Action Classifications</name>
	</scheme>
	<scheme alias="spaward" uri="http://cv.iptc.org/newscodes/spaward/">
		<definition xml:lang="en-GB">Types of award in sport</definition>
		<name xml:lang="en-GB">Award Types</name>
	</scheme>
	<scheme alias="spcompetitionscope" uri="http://cv.iptc.org/newscodes/spcompetitionscope/">
		<definition xml:lang="en-GB">The types of singular or groups of opponents</definition>
		<name xml:lang="en-GB">Scope of Competition</name>
	</scheme>
	<scheme alias="spct" uri="http://cv.iptc.org/newscodes/spct/">
		<definition xml:lang="en-GB">General sports-related concepts such as team, player,
			etc.</definition>
		<name xml:lang="en-GB">Sports Concept</name>
	</scheme>
	<scheme alias="spdistanceunits" uri="http://cv.iptc.org/newscodes/spdistanceunits/">
		<definition xml:lang="en-GB">Units of distance for shot, jump, etc.</definition>
		<name xml:lang="en-GB">Distance Units</name>
	</scheme>
	<scheme alias="spdurationscope" uri="http://cv.iptc.org/newscodes/spdurationscope/">
		<definition xml:lang="en-GB">Variations on standard duration of sporting events</definition>
		<name xml:lang="en-GB">Duration Scope</name>
	</scheme>
	<scheme alias="speventoutcome" uri="http://cv.iptc.org/newscodes/speventoutcome/">
		<definition xml:lang="en-GB">Whether the competitor won, lost, or tied.</definition>
		<name xml:lang="en-GB">Event Outcome</name>
	</scheme>
	<scheme alias="speventoutcometype" uri="http://cv.iptc.org/newscodes/speventoutcometype/">
		<definition xml:lang="en-GB">What type of outcome. Includes overtime, random (eg. by coin toss),
			etc.</definition>
		<name xml:lang="en-GB">Event Outcome Type</name>
	</scheme>
	<scheme alias="speventstatus" uri="http://cv.iptc.org/newscodes/speventstatus/">
		<definition xml:lang="en-GB">The stage of the event, describing whether it has started, is in progress,
			etc.</definition>
		<name xml:lang="en-GB">Event Status</name>
	</scheme>
	<scheme alias="speventstatusreason" uri="http://cv.iptc.org/newscodes/speventstatusreason/">
		<definition xml:lang="en-GB">The reason for the event status. Eg. why the cancellation, postponement,
			etc.</definition>
		<name xml:lang="en-GB">Event Status Reason</name>
	</scheme>
	<scheme alias="speventstyle" uri="http://cv.iptc.org/newscodes/speventstyle/">
		<definition xml:lang="en-GB">Indicates whether competitors are trying to defeat each other or just outdo one
			another.</definition>
		<name xml:lang="en-GB">Event Style</name>
	</scheme>
	<scheme alias="spfixt" uri="http://cv.iptc.org/newscodes/spfixt/">
